{
  "name": "sonos-web",
  "version": "1.0",
  "slug": "sonos-web",
  "description": "Web based controller for Sonos",
  "url": "https://github.com/rickjmatthews/hassio-addons/tree/master/sonos-web",
  "startup": "services",
  "arch": ["aarch64", "amd64", "armv7", "i386"],
  "boot": "auto",
  "hassio_api": true,
  "hassio_role": "default",
  "host_network": true,
  "map": ["config:rw"],
  "privileged": ["NET_ADMIN"],
  "options": {
    "port": "80"
  },
  "schema": {
    "port": "int(0,65535)?"
  }
}
